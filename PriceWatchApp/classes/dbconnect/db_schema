/* SCHEMA for exceldb.
* to reset a table use "Truncate table <tablename>;"

 create table Contact(
   cid INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
   firstname VARCHAR(200),
   lastname VARCHAR(200),
   address VARCHAR(200),
   phone VARCHAR(20),
   email VARCHAR(100) NULL UNIQUE);

create TABLE Rate(
    id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    company VARCHAR(200),
    one FLOAT,
    two FLOAT,
    three FLOAT,
    four FLOAT,
    five FLOAT);